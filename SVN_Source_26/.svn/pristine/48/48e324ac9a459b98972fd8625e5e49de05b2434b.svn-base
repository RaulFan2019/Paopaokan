<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    style="@style/LinearlayouttitleV1"
    xmlns:android="http://schemas.android.com/apk/res/android">

        <com.rey.material.widget.ImageButton
            android:id="@+id/title_bar_left_menu"
            style="@style/MenuLeftBackButton"/>


        <com.rey.material.widget.TextView
            android:id="@+id/tv_title_group_party_all_photo"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:text="@string/Title.GroupPartyAllPhoto"
            style="@style/tvTitleV1"/>

        <com.rey.material.widget.Button
            android:id="@+id/btn_add_photo"
            style="@style/MenuRightButton"
            android:layout_height="match_parent"
            android:layout_gravity="end|center_vertical"
            android:text="@string/Tv.GroupPartyAllPhoto.add"
            android:textColor="#ffffff"
            android:textSize="16sp"/>

</LinearLayout>