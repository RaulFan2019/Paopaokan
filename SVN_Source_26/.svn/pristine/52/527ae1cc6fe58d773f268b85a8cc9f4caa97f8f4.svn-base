package com.app.pao.entity.network;

import java.io.Serializable;

/**
 * 跑步历史心率实例(从网络获取)
 *
 * @author Raul
 *
 */
public class GetWorkoutHeartrateEntity implements Serializable {

	public int bpm;
	public long timeoffset;

	public GetWorkoutHeartrateEntity() {
	}

	public GetWorkoutHeartrateEntity(int bpm, int timeoffset) {
		super();
		this.bpm = bpm;
		this.timeoffset = timeoffset;
	}

	public int getBpm() {
		return bpm;
	}

	public void setBpm(int bpm) {
		this.bpm = bpm;
	}

	public long getTimeoffset() {
		return timeoffset;
	}

	public void setTimeoffset(long timeoffset) {
		this.timeoffset = timeoffset;
	}

}
