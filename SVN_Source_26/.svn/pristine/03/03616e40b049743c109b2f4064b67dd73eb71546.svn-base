<resources>

    <style name="AppTheme" parent="AppBaseTheme">
        <item name="android:listDivider">@drawable/item_divider_bg</item>
    </style>

    <!-- Tab -->
    <style name="TabRootLayout">
        <item name="android:layout_width">1dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:background">?android:attr/selectableItemBackground</item>
        <item name="android:orientation">vertical</item>
        <item name="android:gravity">center_horizontal|center_vertical</item>
    </style>

    <style name="TabRootLayout_TextView">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">10dp</item>
        <item name="android:layout_marginTop">3dp</item>
        <item name="android:textColor">@color/Tab.TextView.Noraml</item>
    </style>

    <style name="TabRootLayout_ImageView">
        <item name="android:layout_width">22dp</item>
        <item name="android:layout_height">22dp</item>
        <item name="android:scaleType">fitCenter</item>
    </style>

    <!-- TextView Style -->
    <style name="TextViewTitle">
        <item name="android:textAppearance">@style/Base.TextAppearance.AppCompat.Title</item>
        <item name="android:textColor">@color/colorPrimaryDark</item>
        <item name="android:textSize">19dp</item>
        <item name="android:layout_marginStart">20dp</item>
        <item name="android:layout_marginEnd">20dp</item>
    </style>

    <style name="TextViewTitle_Key">
        <item name="android:textAppearance">@style/Base.TextAppearance.AppCompat.Title</item>
        <item name="android:textColor">#FF000000</item>
        <item name="android:textSize">19dp</item>
        <item name="android:layout_gravity">center_horizontal</item>
    </style>

    <style name="TextViewTitle_Unit">
        <item name="android:textAppearance">@style/Base.TextAppearance.AppCompat.Title</item>
        <item name="android:textColor">#FF000000</item>
        <item name="android:textSize">19dp</item>
        <item name="android:layout_gravity">center_horizontal</item>
    </style>

    <style name="TextViewBody">
        <item name="android:textAppearance">@style/Base.TextAppearance.AppCompat.Title</item>
        <item name="android:textColor">#FF000000</item>
        <item name="android:textSize">19dp</item>
    </style>

    <style name="TextViewBody_Small">
        <item name="android:textAppearance">@style/Base.TextAppearance.AppCompat.Title</item>
        <item name="android:textColor">#FF000000</item>
        <item name="android:textSize">16dp</item>
    </style>

    <style name="TextViewBody_Key">
        <item name="android:textAppearance">@style/Base.TextAppearance.AppCompat.Title</item>
        <item name="android:textColor">#FF888888</item>
        <item name="android:textSize">12dp</item>
    </style>

    <style name="TextViewBody_Value">
        <item name="android:textAppearance">@style/Base.TextAppearance.AppCompat.Title</item>
        <item name="android:textColor">#FF000000</item>
        <item name="android:textSize">19dp</item>
    </style>

    <style name="TextViewTitle_tip" parent="@style/TextViewTitle">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">0dp</item>
        <item name="android:layout_weight">10</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="TextViewSplashTitle">
        <item name="android:textAppearance">@style/Base.TextAppearance.AppCompat.Title</item>
        <item name="android:textSize">33dp</item>
        <item name="android:textColor">#f06522</item>
    </style>

    <style name="TextViewSplashExplain">
        <item name="android:textAppearance">@style/Base.TextAppearance.AppCompat.Title</item>
        <item name="android:textSize">11dp</item>
        <item name="android:textColor">#888888</item>
    </style>

    <!-- EditText Style-->

    <!-- Button -->
    <style name="ButtonBase_commit" parent="ButtonBase">
        <item name="android:layout_height">44dp</item>
        <item name="android:textSize">16dp</item>
        <item name="android:layout_width">316dp</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:background">@drawable/bg_round_rect</item>
    </style>

    <style name="FlatButtonBase_common" parent="FlatButtonBase">
        <item name="android:textSize">14dp</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/Btn.FlatButtonBase.common.Text</item>
    </style>

    <style name="FlatButtonBase_frame" parent="Material.Drawable.Ripple.Touch.Light">
        <item name="android:textSize">14dp</item>
        <item name="android:background">@null</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="rd_cornerRadius">4dp</item>
    </style>

    <style name="ButtonBase_common" parent="ButtonBase">
        <item name="android:textSize">19dp</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <!-- ToolBar -->
    <style name="ToolbarBase">
        <item name="android:layout_height">?attr/actionBarSize</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:background">@color/colorPrimaryDark</item>
        <item name="android:textColor">@color/white</item>
    </style>


    <!-- dialog -->
    <style name="SimpleDialogLight" parent="Material.App.Dialog.Simple.Light">
        <item name="di_inAnimation">@anim/anim_scale_in</item>
        <item name="di_outAnimation">@anim/anim_scale_out</item>
        <item name="di_actionTextColor">?attr/colorPrimary</item>
    </style>

    <!-- Linearlayout -->
    <style name="LineViewBase_Settings">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:orientation">vertical</item>
        <item name="android:paddingLeft">@dimen/line_left_margin</item>
        <item name="android:paddingRight">@dimen/line_right_margin</item>
        <item name="android:gravity">center_vertical</item>
    </style>

    <style name="LineViewBase_Settings_single">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">52dp</item>
        <item name="android:orientation">vertical</item>
        <item name="android:gravity">center_vertical</item>
    </style>

    <style name="LinearlayoutBase_Login_weight" parent="@style/LinearlayoutBase">
        <item name="android:gravity">center_horizontal</item>
        <item name="android:layout_height">0dp</item>
        <item name="android:layout_weight">32</item>
    </style>

    <style name="LineViewBase_Best" parent="@style/LineViewBase">
        <item name="android:layout_height">52dp</item>
    </style>


    <!-- toolbar -->

    <style name="AppTheme_AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="ToolbarlayouTransparentText">
        <item name="android:textColor">#00000000</item>
    </style>

    <!-- load view -->
    <style name="LoadView">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:background">@android:color/transparent</item>
        <item name="android:gravity">center</item>
        <item name="android:orientation">vertical</item>
    </style>

    <style name="LoadView_Tv">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:textColor">@color/colorPrimaryDark</item>
        <item name="android:text">@string/Load.loading</item>
        <item name="android:textSize">19dp</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:layout_marginTop">5dp</item>
    </style>

    <style name="LoadView_Pv" parent="@style/CircularProgressViewBase">
        <item name="android:layout_width">45dp</item>
        <item name="android:layout_height">45dp</item>
        <item name="android:layout_gravity">center</item>
    </style>


    <!-- divider -->
    <style name="DividerBase_vertical">
        <item name="android:layout_width">1px</item>
        <item name="android:background">#d8d8d8</item>
    </style>

    <style name="DividerBase_horizontal">
        <item name="android:layout_height">1px</item>
        <item name="android:background">#d8d8d8</item>
    </style>

    <style name="DividerBase_card_vertical">
        <item name="android:layout_width">1px</item>
        <item name="android:background">#e7e7e7</item>
    </style>

    <style name="DividerBase_card_horizontal">
        <item name="android:layout_height">1px</item>
        <item name="android:background">#e7e7e7</item>
    </style>

    <!-- common EditLayout -->
    <style name="EditLayout">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:paddingLeft">@dimen/line_left_margin</item>
        <item name="android:paddingRight">@dimen/line_right_margin</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="EditLayout_InputLayout">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">14dp</item>
        <item name="android:layout_marginBottom">14dp</item>
        <item name="android:gravity">center_vertical</item>
    </style>

    <style name="EditLayout_Title">
        <item name="android:layout_width">50dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/EditLayout.Title</item>
        <item name="android:textSize">15dp</item>
    </style>

    <style name="EditLayout_Title_disable">
        <item name="android:layout_width">50dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/EditLayout.Title.disable</item>
        <item name="android:textSize">15dp</item>
    </style>

    <style name="EditLayout_Edittext">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:gravity">start|center_vertical</item>
        <item name="android:background">@null</item>
        <item name="android:textSize">15dp</item>
    </style>

    <style name="EditLayout_Edittext_Psw" parent="EditLayout_Edittext">
        <item name="android:inputType">textPassword</item>
        <item name="android:maxLength">16</item>
    </style>

    <style name="EditLayout_Edittext_Phone" parent="EditLayout_Edittext">
        <item name="android:inputType">phone</item>
        <item name="android:maxLength">13</item>
    </style>

    <style name="EditLayout_Edittext_SMS_Code" parent="EditLayout_Edittext">
        <item name="android:inputType">number</item>
        <item name="android:maxLength">4</item>
    </style>

    <style name="EditLayout_BottomLine" parent="@style/DividerBase_horizontal">
        <item name="android:layout_width">match_parent</item>
    </style>

    <style name="EditLayout_CuttingLine" parent="@style/DividerBase_vertical">
        <item name="android:layout_height">25dp</item>
        <item name="android:layout_marginStart">20dp</item>
        <item name="android:layout_marginEnd">20dp</item>
    </style>

    <!--   USE FOR  V1  UI -->
    <!-- dialog -->
    <style name="AutocloseDialogTitleV1" parent="android:style/TextAppearance.DialogWindowTitle">
        <item name="android:gravity">left</item>
    </style>

    <style name="DialogThemeV1" parent="android:style/Theme.Dialog">
        <item name="android:windowTitleStyle">@style/AutocloseDialogTitleV1</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowBackground">@null</item>
    </style>


    <style name="edit_base">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:textSize">19dp</item>
