<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
              xmlns:android="http://schemas.android.com/apk/res/android"
              android:layout_width="match_parent"
              android:layout_height="66dp"
              android:orientation="vertical">
    <LinearLayout
        style="@style/ListViewItem"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        android:gravity="center_vertical"
        android:orientation="horizontal">

        <com.app.pao.ui.widget.CircularImage
            android:id="@+id/ci_member_photo"
            android:layout_width="38dp"
            android:layout_height="38dp"
            android:background="@drawable/icon_user_photo"/>
        <!--android:src="@drawable/icon_user_photo"/>-->


        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="10dp"
            android:layout_weight="1"
            android:gravity="center_vertical"
            android:orientation="horizontal">


            <TextView
                android:id="@+id/tv_member_nickname"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>

            <ImageView
                android:id="@+id/iv_member_gander"
                android:layout_width="12dp"
                android:layout_height="12dp"
                android:layout_marginStart="6dp"/>

        </LinearLayout>


        <ImageView
            android:id="@+id/iv_tag_check"
            android:layout_width="20dp"
            android:layout_height="20dp"/>
    </LinearLayout>
    <View
        style="@style/DividerBase_horizontal"
        android:layout_width="match_parent"/>
</LinearLayout>