<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >

    <item android:id="@android:id/background">
        <shape>
            <size android:height="5dp"
                android:width="5dp"></size>
            <stroke android:color="#00000000"
                    android:width="10dp"></stroke>
            <gradient
                android:startColor="#555555"
                android:centerColor="#555555"
                android:endColor="#555555"
                android:centerY="0.45"
                android:angle="270"/>
        </shape>
    </item>

    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <size android:height="10dp"
                    android:width="10dp"></size>
                <stroke android:color="#00000000"
                    android:width="10dp"
                    ></stroke>
                <gradient
                    android:startColor="#f06522"
                    android:centerColor="#f06522"
                    android:endColor="#f06522"
                    android:centerY="0.45"
                    android:angle="270"/>
            </shape>
        </clip>
    </item>

</layer-list>