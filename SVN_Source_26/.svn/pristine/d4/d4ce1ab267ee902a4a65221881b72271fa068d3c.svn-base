package com.app.pao.entity.event;

import java.io.Serializable;

/**
 * EventBus 传跑步距离
 *
 * @author zhoujun
 */
public class EventRuningLocation implements Serializable{

    public float length;// 距离
    public int speed;// 配速
    public float Calorie;// 卡路里

    public float getLength() {
        return length;
    }

    public void setLength(float length) {
        this.length = length;
    }

    public int getSpeed() {
        return speed;
    }

    public void setSpeed(int speed) {
        this.speed = speed;
    }

    public float getCalorie() {
        return Calorie;
    }

    public void setCalorie(float calorie) {
        Calorie = calorie;
    }

    public EventRuningLocation(float length, int speed, float calorie) {
        this.length = length;
        this.speed = speed;
        this.Calorie = calorie;
    }

    public EventRuningLocation(float length, int speed) {
        this.length = length;
        this.speed = speed;
    }
}
