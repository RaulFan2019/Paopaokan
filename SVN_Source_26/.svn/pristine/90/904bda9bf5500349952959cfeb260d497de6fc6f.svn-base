package com.app.pao.entity.model;

import com.app.pao.entity.network.GetRaceLiveListResult;

/**
 * Created by Raul on 2015/12/5.
 * 赛事直播选手列表模型
 */
public class RacePersonEntity {

    private GetRaceLiveListResult.RaceUserEntity userEntity;
    private int raceType;

    public RacePersonEntity() {

    }

    public RacePersonEntity(GetRaceLiveListResult.RaceUserEntity userEntity, int raceType) {
        super();
        this.userEntity = userEntity;
        this.raceType = raceType;
    }

    public GetRaceLiveListResult.RaceUserEntity getUserEntity() {
        return userEntity;
    }

    public void setUserEntity(GetRaceLiveListResult.RaceUserEntity userEntity) {
        this.userEntity = userEntity;
    }

    public int getRaceType() {
        return raceType;
    }

    public void setRaceType(int raceType) {
        this.raceType = raceType;
    }
}
