package com.app.pao.entity.event;

import com.app.pao.entity.network.GetJpushMessageResult;

/**
 * jPush事件
 *
 * @author Raul
 *
 */
public class EventJpush {

	private int msgType;
	private int notifactionId;
	private int groupId;

	public EventJpush() {

	}

	public EventJpush(int msgType, int notifactionId) {
		super();
		this.msgType = msgType;
		this.notifactionId = notifactionId;
	}

	public EventJpush(int msgType, int notifactionId, int groupId) {
		super();
		this.msgType = msgType;
		this.notifactionId = notifactionId;
		this.groupId = groupId;

	}

	public int getMsgType() {
		return msgType;
	}

	public void setMsgType(int msgType) {
		this.msgType = msgType;
	}

	public int getNotifactionId() {
		return notifactionId;
	}

	public void setNotifactionId(int notifactionId) {
		this.notifactionId = notifactionId;
	}

	public int getGroupId() {
		return groupId;
	}

	public void setGroupId(int groupId) {
		this.groupId = groupId;
	}

}
