package com.app.pao.entity.network;

import java.util.List;

/**
 * Created by Administrator on 2016/1/14.
 *
 * 个人跑团名片信息
 */
public class GetGroupMemberInforResult {
    private String alias;
    private int id;
    private String nickname;
    private int gender;
    private String birthdate;
    private String avatar;
    private int signuppartycount;   //报名活动数量
    private int checkinpartycount;  //签到的活动数量
    private String laststarttime;   //最近一次锻炼时间
    private int role;
    private List<Label> label;

    public String getAlias() {
        return alias;
    }

    public void setAlias(String alias) {
        this.alias = alias;
    }

    public int getId() {
        return id;
    }

    public void setId(int id) {
        this.id = id;
    }

    public String getNickname() {
        return nickname;
    }

    public void setNickname(String nickname) {
        this.nickname = nickname;
    }

    public int getGender() {
        return gender;
    }

    public void setGender(int gender) {
        this.gender = gender;
    }

    public String getBirthdate() {
        return birthdate;
    }

    public void setBirthdate(String birthdate) {
        this.birthdate = birthdate;
    }

    public String getAvatar() {
        return avatar;
    }

    public void setAvatar(String avatar) {
        this.avatar = avatar;
    }

    public int getSignuppartycount() {
        return signuppartycount;
    }

    public void setSignuppartycount(int signuppartycount) {
        this.signuppartycount = signuppartycount;
    }

    public int getCheckinpartycount() {
        return checkinpartycount;
    }

    public void setCheckinpartycount(int checkinpartycount) {
        this.checkinpartycount = checkinpartycount;
    }

    public String getLaststarttime() {
        return laststarttime;
    }

    public void setLaststarttime(String laststarttime) {
        this.laststarttime = laststarttime;
    }

    public int getRole() {
        return role;
    }

    public void setRole(int role) {
        this.role = role;
    }

    public List<Label> getLabel() {
        return label;
    }

    public void setLabel(List<Label> label) {
        this.label = label;
    }

    public class Label {
        private int id;
        private String name;

        public int getId() {
            return id;
        }

        public void setId(int id) {
            this.id = id;
        }

        public String getName() {
            return name;
        }

        public void setName(String name) {
            this.name = name;
        }
    }
}
