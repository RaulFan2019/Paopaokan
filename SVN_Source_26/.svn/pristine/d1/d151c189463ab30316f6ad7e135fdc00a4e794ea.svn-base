<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/ll_medal_change_dialog"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#888888">

    <View
        android:id="@+id/v_gender"
        android:layout_width="match_parent"
        android:layout_height="470dp"
        android:layout_gravity="bottom"
        android:background="@drawable/bg_new_record_men" />

    <View
        android:id="@+id/v_medal"
        android:layout_width="match_parent"
        android:layout_height="470dp"
        android:layout_gravity="bottom|center_horizontal"
        android:background="@drawable/icon_copper_fastest_5_big" />

    <FrameLayout
        android:id="@+id/race_layout"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <com.app.pao.ui.widget.ViewFlow
            android:id="@+id/vf_medal_change"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent" />

        <com.app.pao.ui.widget.CircleFlowIndicator
            android:id="@+id/cfi_medal_change_point"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal|top"
            android:layout_marginTop="21dp"
            android:padding="2dp" />
    </FrameLayout>

    <View
        android:id="@+id/v_finish"
        android:layout_width="19dp"
        android:layout_height="19dp"
        android:layout_gravity="right"
        android:layout_marginRight="15dp"
        android:layout_marginTop="15dp"
        android:background="@drawable/icon_finish_white" />

</FrameLayout>