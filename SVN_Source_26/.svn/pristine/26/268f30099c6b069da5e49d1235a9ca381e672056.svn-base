package com.app.pao.entity.event;

import com.app.pao.entity.network.GetDynamicListResult;

/**
 * Created by Raul on 2016/1/12.
 * 动态内容点击事件
 */
public class EventDynamicClick {

    public int position;
    public GetDynamicListResult item;

    public EventDynamicClick(int position,GetDynamicListResult item) {
        this.item = item;
        this.position = position;
    }

    public GetDynamicListResult getItem() {
        return item;
    }

    public void setItem(GetDynamicListResult item) {
        this.item = item;
    }

    public int getPosition() {
        return position;
    }

    public void setPosition(int position) {
        this.position = position;
    }
}
