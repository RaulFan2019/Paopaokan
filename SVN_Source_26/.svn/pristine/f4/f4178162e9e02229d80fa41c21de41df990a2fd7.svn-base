package com.app.pao.entity.model;

/**
 * Created by Raul on 2015/12/17.
 * 环行图使用对象
 */
public class PieChartEntity {

    private int persent;
    private int color;


    public PieChartEntity() {

    }

    public PieChartEntity(int persent, int color) {
        this.persent = persent;
        this.color = color;
    }

    public int getPersent() {
        return persent;
    }

    public void setPersent(int persent) {
        this.persent = persent;
    }

    public int getColor() {
        return color;
    }

    public void setColor(int color) {
        this.color = color;
    }
}
